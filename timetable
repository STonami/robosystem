import sys

# 時間割データ
timetable = {
    "月": "月曜 1.なし 2.なし 3.なし 4.ロボット設計製作論3 5.ロボット設計製作論3 6. ロボット設計製作論3 7.ロボット設計製作論3 8.なし 9.なし 10.なし",
    "火": "火曜 1.英語理解4a 2.英語理解4a 3.なし 4.なし 5.なし 6.未来ロボティクス総 合セミナー 7.未来ロボティクス総合セミナー 8.なし 9.なし 10.なし",
    "水": "水曜 1.数値解析学1 2.数値解析学1 3.センサ工学 4.センサ工学 5.なし 6.なし 7.なし 8.なし 9.なし 10.なし",
    "木": "木曜 1.なし 2.なし 3.ロボットシステム学 4.ロボットシステム学 5.なし 6.なし 7.なし 8.なし 9.なし 10.なし",
    "金": "金曜 1.なし 2.信号処理論 3.信号処理論 4.なし 5.遊びと社会 6.遊びと社会 7.英語表現3a 8.英語表現3a 9.なし 10.なし",
}

# メイン処理
def main():
    # 標準入力からすべての行を取得
    if not sys.stdin.isatty():
        lines = sys.stdin.read().strip().splitlines()
    elif len(sys.argv) > 1:
        lines = [sys.argv[1]]
    else:
        print("エラー: 有効な曜日を指定してください。月、火、水、木、金のいずれかを入力してください。")
        return

    # 各行を処理
    for line in lines:
        line = line.strip()
        if line in timetable:
            print(timetable[line])
        else:
            print("エラー: 有効な曜日を指定してください。月、火、水、木、金のいずれかを入力してください。")

if __name__ == "__main__":
    main()

